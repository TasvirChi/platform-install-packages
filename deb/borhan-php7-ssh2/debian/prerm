#!/bin/sh

set -e

dpkg-maintscript-helper mv_conffile /opt/borhan/app/configurations/php/conf.d/ssh2.ini \
    /opt/borhan/app/configurations/php/mods-available/ssh2.ini 0.12-1 -- "$@"
rm -f /opt/borhan/app/configurations/php/conf.d/ssh2.ini

[ "$1" = "remove" ] && /opt/borhan/bin/php7dismod ssh2

#DEBHELPER#

exit 0
