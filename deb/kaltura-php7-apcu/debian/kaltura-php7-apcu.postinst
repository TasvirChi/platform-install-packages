#!/bin/sh

set -e

dpkg-maintscript-helper mv_conffile /opt/borhan/app/configurations/php/conf.d/apcu.ini \
    /opt/borhan/app/configurations/php/mods-available/apcu.ini 5.1.0-1 -- "$@"
rm -f /opt/borhan/app/configurations/php/conf.d/apcu.ini

[ "$1" = "configure" ] && /opt/borhan/bin/php7enmod apcu

#DEBHELPER#

exit 0
